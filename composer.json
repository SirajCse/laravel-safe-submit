{
    "name": "SirajCSE/laravel-safe-submit",
    "description": "Preventing Duplicate Form  Submits  in laravel framework",
    "authors": [
        {
            "name": "Md. Siraj-Ud-Doulla",
            "email": "sirajuddoulla@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "SirajCSE\\LaravelSafeSubmit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SirajCSE\\LaravelSafeSubmit\\SafeSubmitServiceProvider"
            ],
            "aliases": {
                "SafeSubmit": "SirajCSE\\LaravelSafeSubmit\\SafeSubmit::class"
            }
        }
    }
}
